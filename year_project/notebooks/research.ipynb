    "import torch\n",
    "\n",
    "y_pred=torch.tensor([[4,5,6],[1,3,6]])\n",
    "y=torch.tensor([2,3])\n",
    "top_pred = y_pred.argmax(1, keepdim=True)\n",
    "print(top_pred,\"top_pred\")\n",
    "\n",
    "correct = top_pred.eq(y.view_as(top_pred)).sum()\n",
    "print(correct)"
